        "vue-router": "^4.5.0"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.1.2",
        "@ti-platform/aide-build-tools": "workspace:*",
        "@types/node": "^22.14.0",
        "@vitejs/plugin-vue": "^5.2.3",
        "defu": "^6.1.4",
