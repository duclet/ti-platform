        "@quasar/vite-plugin": "^1.1.0",
        "@ti-platform/aide-build-tools": "workspace:*",
        "@types/node": "^16.0.0",
        "@vitejs/plugin-vue": "^3.0.0",
        "eslint": "^8.25.0",
        "prettier": "^2.7.0",
        "typedoc": "^0.23.13",
        "typedoc-plugin-markdown": "^3.13.0",
        "typescript": "^4.8.0",
        "vite": "^3.1.0",
        "vue-tsc": "^1.0.8"
