{
    "name": "@ti-platform/aide-quasar",
    "version": "2.2.2",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./src/index.ts",
    "files": [
        "dist/",
        "src/",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build:docs": "run-typedoc -i ./src/index.docs.ts -v 2>&1",
        "build:src": "vite build",
        "clean": "rm -rf ./dist",
        "prod": "pnpm clean && pnpm build:src && pnpm build:docs",
        "dev": "pnpm clean && pnpm build:src --watch",
        "build": "pnpm prod",
        "preview": "pnpm vite -c vite-preview.config.ts"
    },
    "dependencies": {
        "@s-libs/micro-dash": "^16.0.0",
        "@ti-platform/aide": "workspace:*",
        "@ti-platform/aide-vue": "workspace:*",
        "@vueuse/core": "^10.2.0",
        "quasar": "^2.12.0",
        "vue": "^3.3.4",
        "vue-router": "^4.2.2"
    },
    "devDependencies": {
        "@quasar/extras": "^1.16.4",
        "@quasar/vite-plugin": "^1.3.3",
        "@ti-platform/aide-build-tools": "workspace:*",
        "@types/node": "^16.0.0",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "@typescript-eslint/parser": "^5.60.0",
        "@vitejs/plugin-vue": "^4.2.3",
        "eslint": "^8.43.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-vue": "^9.15.0",
        "prettier": "^2.8.8",
        "sass": "1.32.13",
        "typedoc": "^0.24.8",
        "typedoc-plugin-markdown": "^3.15.3",
        "typescript": "^5.0.4",
        "vite": "^4.3.9",
        "vite-plugin-dts": "^3.1.1",
        "vue-docgen-cli": "^4.67.0",
        "vue-tsc": "^1.8.1"
    },
    "typedocOptions": {
        "plugin": [
            "typedoc-plugin-markdown"
        ]
    }
}
