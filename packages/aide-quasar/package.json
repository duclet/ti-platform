{
    "name": "@ti-platform/aide-quasar",
    "version": "1.0.2",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./src/index.ts",
    "files": [
        "dist/",
        "src/",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
