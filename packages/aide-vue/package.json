{
    "name": "@ti-platform/aide-vue",
    "version": "0.3.0",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/",
        "src/",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build:docs": "run-typedoc",
        "build:src": "tsup",
        "clean": "rm -rf ./dist",
        "lint": "run-linter -d ./src -e .ts -g -k -c 'pnpm tsc --noEmit'",
        "prod": "pnpm clean && pnpm lint && pnpm build:src --minify && pnpm build:docs",
        "dev": "pnpm clean && pnpm lint -w -c 'pnpm build:src'",
        "build": "pnpm prod"
    },
    "dependencies": {
        "@ti-platform/aide": "workspace:*",
        "vue": "^3.2.0"
    },
    "optionalDependencies": {
        "@vueuse/core": "^9.3.0"
    },
    "devDependencies": {
        "@ti-platform/aide-build-tools": "workspace:*",
        "@types/node": "^16.0.0",
        "eslint": "^8.25.0",
        "prettier": "^2.7.0",
        "tsup": "^6.2.3",
        "typedoc": "^0.23.13",
        "typedoc-plugin-markdown": "^3.13.0",
        "typescript": "^4.8.0"
    }
}
