{
    "name": "@ti-platform/aide-vue",
    "version": "0.1.6",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/",
        "src/"
    ],
    "config": {
        "linterArgs": "-d ./src -e .ts -g"
    },
    "scripts": {
        "build": "rm -rf ./dist && pnpm run-linter -- -k && tsc -d",
        "dev": "rm -rf ./dist && pnpm run-linter -- -k -w -c 'tsc --incremental'",
        "run-linter": "run-linter.js $npm_package_config_linterArgs"
    },
    "dependencies": {
        "@ti-platform/aide": "workspace:*",
        "vue": "3.2.23"
    },
    "devDependencies": {
        "@ti-platform/aide-build-tools": "workspace:*",
        "@types/node": "16.11.12",
        "eslint": "8.4.1",
        "prettier": "2.5.1",
        "typescript": "4.5.2",
        "vite": "2.6.14"
    }
}
